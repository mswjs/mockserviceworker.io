---
import type { ComponentProps } from 'astro/types'
import BaseHeader from '@mswjs/shared/components/Header.astro'

import mswLogo from '../images/msw.svg'

type Props = Partial<ComponentProps<typeof BaseHeader>>

const props = Astro.props
---

<BaseHeader
  {...props}
  logo={mswLogo}
  links={[
    {
      label: 'Docs',
      href: '/docs'
    },
    {
      label: 'Ecosystem',
      href: '/ecosystem'
    },
    {
      label: 'Blog',
      href: '/blog'
    }
  ]}
/>